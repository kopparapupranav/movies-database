CREATE TABLE ACTOR(
ACT_ID NUMBER(3),
ACT_NAME VARCHAR(20),
ACTRESS_NAME VARCHAR(20),
MOVIE_NAME VARCAHR(20),
DIRECTOR_NAME VARCHAR(20),
RELEASE_YEAR NUMBER(4),
PRIMARY KEY (ACT_ID));

